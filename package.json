{
  "name": "@rolster/typescript-utils",
  "version": "4.1.0",
  "description": "Utilities package compatible with Typescript projects.",
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "unpkg": "dist/es/index.js",
  "types": "dist/esm/index.d.ts",
  "license": "MIT",
  "author": "Rolster Technology <rolster.developments@gmail.com>",
  "contributors": [
    {
      "name": "Daniel Andr√©s Castillo Pedroza",
      "email": "ing.dacastillop@gmail.com"
    }
  ],
  "files": [
    "dist/",
    "constants/"
  ],
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "node": "./dist/cjs/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    },
    "./array": {
      "types": "./dist/esm/array.d.ts",
      "node": "./dist/cjs/array.js",
      "require": "./dist/cjs/array.js",
      "default": "./dist/esm/array.js"
    },
    "./date": {
      "types": "./dist/esm/date.d.ts",
      "node": "./dist/cjs/date.js",
      "require": "./dist/cjs/date.js",
      "default": "./dist/esm/date.js"
    },
    "./string": {
      "types": "./dist/esm/string.d.ts",
      "node": "./dist/cjs/string.js",
      "require": "./dist/cjs/string.js",
      "default": "./dist/esm/string.js"
    }
  },
  "scripts": {
    "prettier": "prettier . --write",
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc -p tsconfig.json && rollup -c rollup.config.js",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "@rolster/typescript-types": "^4.0.0",
    "@types/jest": "^29.5.1",
    "jest": "^29.5.0",
    "prettier": "^3.0.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.4.0",
    "typescript": "^4.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rolster-developments/typescript-utils.git"
  }
}
